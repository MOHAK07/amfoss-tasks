#!/bin/python3

import sys


t = int(input().strip())
for a0 in range(t):
    n = int(input())
    

    #(3, 6, 9, 12, 15, 18, 21, 24, 27, 30 ....,3*n3) == 3*(1, 2, 3, 4, 5, 6, 7, ...., n3)
    #(5, 10,       15, 20, 25,         30, ...., 5*5n) == 5*(1, 2, 3, 4, 5, 6, ..., n5)
    #(15, 30, 45, ....) == 15*(1, 2, 3, 4, 5, ..., n15)

    n3 = (n-1)//3
    n5 = (n-1)//5
    n15 = (n-1)//15

    #sum i , for i = 1 to m == m(m+1)/2
    val3 = 3*n3*(n3 + 1)//2
    val5 = 5*n5*(n5 + 1)//2
    val15 = 15*n15*(n15 + 1)//2

    total = val3 + val5 - val15
    print(total)
